{
    "name": "axguowen/think-framework",
    "description": "The ThinkPHP Framework For PHP5.6",
    "keywords": [
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0",
        "psr/container": "1.0.0",
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "axguowen/think-orm": "^2.0",
        "axguowen/think-helper": "^3.1.1"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "think\\": "src/think/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
